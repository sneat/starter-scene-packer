{
  "id": "starter-scene-packer",
  "title": "Starter for Scene Packer",
  "description": "A starting point for creating modules with Scene Packer functionality.",
  "version": "1.0.0",
  "compatibility": {
    "minimum": "12",
    "verified": "13"
  },
  "authors": [
    {
      "name": "Blair McMillan",
      "discord": "blair#9056",
      "url": "https://www.patreon.com/blairm"
    }
  ],
  "media": [
    {
      "type": "setup",
