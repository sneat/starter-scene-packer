  "packFolders": [
    {
      "name": "Starter Scene Packer",
      "sorting": "m",
      "color": "#0000ff",
      "packs": [
        "actors",
        "cards",
        "items",
        "journals",
        "macros",
        "maps",
        "playlists",
        "rolltables"
      ],
      "folders": [
        {
          "name": "Adventure",
          "sorting": "a",
          "color": "#ff0000",
          "packs": [
            "adventure"
